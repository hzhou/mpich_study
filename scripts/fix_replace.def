include: macros/patch.def
page: t, patch_frame
    module: perl
    patch: REPLACE

#---------------------------------------- 
subcode: grep_REPLACE
    $call grep_l, 'MPI_AINT_FMT_.*_SPEC'

subcode: filter_lines_REPLACE
    $if $(l)=~/MPI_AINT_FMT_(.*)_SPEC/
        $cnt++
        $(l) =~s/MPI_AINT_FMT_DEC_SPEC/MPIR_FMT_AINT_d/g
        $(l) =~s/MPI_AINT_FMT_HEX_SPEC/MPIR_FMT_AINT_x/g

