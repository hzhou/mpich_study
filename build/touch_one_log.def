subcode: _autoload
    $call set_macros

page: t
    module: general

    lo_list: $(lo_list)
    obj_list: $(obj_list)

perlcode: set_macros
    $(set:pre=lib_libmpi_la)
    $call @parse_Makefile
    my $lo_list = get_list("am_lib_libmpi_la_OBJECTS")
    my $fort_lo_list=get_list("am_lib_libmpifort_la_OBJECTS")
    my $cnt = @$lo_list
    my $cnt_fort = @$fort_lo_list

    $print lo_list: $cnt objects
    $print fort_lo_list: $cnt_fort objects

    my $macros=$MyDef::def->{macros}
    $macros->{lo_list} = join(" ", @$lo_list)

    my (@obj_list, @obj_dot_list)
    $foreach $o in @$lo_list
        $if $o=~/(.*\/)(.*)\.lo/
            push @obj_list, "$1/$2.o"
            push @obj_dot_list, "$1.libs/$2.o"
    $macros->{obj_list} = join(" ", @obj_list)
    $macros->{obj_dot_list} = join(" ", @obj_dot_list)
    $call @sub_get_list
    # --------------------
    subcode: parse_Makefile
        my %objects
        my $tlist
        &call open_r, Makefile
            $if /^(am_lib_.*_la_OBJECTS|am__objects_\d+)\s*=\s*(.*)/ -> $a, $b
                $tlist=[]
                $objects{$a} = $tlist

                $call add_b_to_tlist
            $elif $tlist
                $if /\s*(.*)/ -> $b
                    $call add_b_to_tlist
    subcode: sub_get_list
        $sub get_list($key)
            my @t
            my $tlist = $objects{$key}
            # $call @debug_get_list
            $foreach $t in @{$objects{$key}}
                $if $t=~/\$\((\w+)\)/
                    my $L = get_list($1)
                    push @t, @$L
                $else
                    push @t, $t
            return \@t
        subcode: debug_get_list
            $(if:1)
                $print $key: $tlist
            $(else)
                $if $tlist
                    $print $key: @$tlist
                $else
                    $print $key undefined
    # -------------------
    subcode: open_r(f)
        open In, "$(f)" or die "Can't open $(f)\n"
        $while <In>
            BLOCK
        close In

    subcode: add_b_to_tlist
        my $done=1
        $if $b=~/\\$/
            $done = 0
            $b=~s/\s*\\$//

        $if $b
            push @$tlist, split /\s+/, $b
        $if $done
            undef $tlist

